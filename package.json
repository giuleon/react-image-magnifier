{
  "name": "react-image-magnifier-lens",
  "version": "1.1.6",
  "description": "Magnify an image through a lens",
  "main": "dist/react.image.magnifier.js",
  "types": "dist/indexPackage.d.ts",
  "style": "dist/react.image.magnifier.css",
  "scripts": {
    "start": "set NODE_ENV=development && webpack-dev-server --open --config ./webpack/webpack.config.dev.js",
    "prebuild": "npm install",
    "build": "set NODE_ENV=production && webpack --progress --config ./webpack/webpack.config.prod.js",
    "stats": "set NODE_ENV=production && webpack --progress --config ./webpack/webpack.config.stats.js --profile --json > ./webpack/stats/stats.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/giuleon/react-image-magnifier.git"
  },
  "keywords": [
    "React",
    "Image",
    "Magnifier"
  ],
  "author": {
    "name": "Giuliano De Luca",
    "email": "deluca.giuliano@gmail.com",
    "url": "http://www.delucagiuliano.com"
  },
  "homepage": "https://github.com/giuleon/react-image-magnifier",
  "dependencies": {
    "styled-components": "2.2.4",
    "@types/react": "^16.0.5",
    "@types/react-dom": "^15.5.4",
    "react": "^15.6.1",
    "react-dom": "^15.6.1"
  },
  "devDependencies": {
    "@types/node": "8.0.53",
    "@types/react": "16.0.25",
    "@types/react-dom": "16.0.3",
    "css-loader": "0.28.7",
    "extract-text-webpack-plugin": "3.0.2",
    "path": "0.12.7",
    "prop-types": "15.6.0",
    "react": "15.x.x || 16.x.x",
    "react-container-dimensions": "1.3.3",
    "react-dom": "15.x.x || 16.x.x",
    "react-hot-loader": "3.1.3",
    "source-map-loader": "0.2.3",
    "style-loader": "0.19.0",
    "stylelint": "8.3.1",
    "ts-loader": "3.2.0",
    "tslint": "5.8.0",
    "tslint-react": "3.2.0",
    "typescript": "2.6.2",
    "webpack": "3.8.1",
    "webpack-dev-server": "2.9.5",
    "webpack-visualizer-plugin": "0.1.11"
  }
}
